{
	"name": "vscode-mystock",
	"displayName": "vscode-mystock",
	"description": "vscode-mystock",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.closeLook",
				"title": "Close Look"
			},
			{
				"command": "extension.openLook",
				"title": "Open Look"
			}
		],
		"keybindings": [
			{
				"command": "extension.closeLook",
				"key": "ctrl+W",
				"when": "editorFocus"
			},
			{
				"command": "extension.openLook",
				"key": "ctrl+Y",
				"when": "editorFocus"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
        "vscode-test": "^1.0.2",
        "vscode": "^1.1.36"
	},
	"dependencies": {
		"iconv-lite": "^0.5.0",
		"request": "^2.88.0",
		"vscode": "^1.1.36"
	}
}
